<AnchorPane fx:id="context" prefHeight="275.0" prefWidth="337.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.insitute.iimanage.controller.CodeVerificationForm">
